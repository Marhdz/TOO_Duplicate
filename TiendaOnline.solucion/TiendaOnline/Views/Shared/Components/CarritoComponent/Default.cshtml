@model CarritoVM

@if (Model.Carrito.ProdCarrito.Count > 0) 
{
    <li>
        <a asp-controller="Carrito">
            <span class="glyphicon glyphicon-shopping-cart"></span>
            <span id="carrito-status">
                @Model.Carrito.ProdCarrito.Count
            </span>
        </a>
    </li>
}

